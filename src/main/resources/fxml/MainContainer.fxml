<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" fx:id="MainContent">
         <top>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
      		      <Label fx:id="title" alignment="CENTER" prefHeight="16.0" prefWidth="600.0" text="Cadastro de Clientes" GridPane.rowIndex="1" />
               	 
               		<MenuBar>
      		        <menus>
      		          <Menu mnemonicParsing="false" text="Vendas" />
      		          <Menu mnemonicParsing="false" text="Clientes" />
      		          <Menu mnemonicParsing="false" text="Produtos" />
      		          <Menu mnemonicParsing="false" text="Ajuda">
      		            <items>
      		              <MenuItem mnemonicParsing="false" text="Sobre" />
      		            </items>
      		          </Menu>
      		        </menus>
      		      </MenuBar>
               </children>
            </GridPane>
         </top>
         <center>
      		<TabPane prefHeight="353.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
              <tabs>
                <Tab text="Registros">
                  <content>
                 	     <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                	     	<children>
               	        	<TableView layoutX="200.0" layoutY="153.0" prefHeight="200.0" prefWidth="200.0">
               	            	<columns>
               	                	<TableColumn prefWidth="75.0" text="C1" />
               	                	<TableColumn prefWidth="75.0" text="C2" />
               	                </columns>
               	            </TableView>
               	        </children>
               	     </AnchorPane>
                  </content>
                </Tab>
                <Tab text="Novo Cliente">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                          <children>
                             <Label layoutX="14.0" layoutY="22.0" text="Nome" />
                          </children></AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
